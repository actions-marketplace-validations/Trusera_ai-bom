{
  "name": "trusera-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for monitoring AI agents with Trusera - intercept HTTP (fetch, axios, undici), evaluate Cedar policies",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Trusera/ai-bom"
  },
  "keywords": [
    "ai",
    "agents",
    "monitoring",
    "security",
    "interceptor",
    "observability",
    "langchain",
    "ai-security",
    "agent-monitoring",
    "policy-enforcement"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "axios": ">=1.0.0",
    "langchain": ">=0.1.0",
    "undici": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "langchain": {
      "optional": true
    },
    "axios": {
      "optional": true
    },
    "undici": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "axios": "^1.13.5",
    "eslint": "^8.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "undici": "^7.22.0",
    "vitest": "^1.0.0"
  }
}
